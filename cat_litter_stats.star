# ======================================
# Cat Name Helper
# ======================================
def get_cat_order_list(cat_order):
    cat_order_list = []
    string_raw = cat_order
    if string_raw:
        for block in string_raw.split(","):
            cat_order_list.append(block.strip())
    return cat_order_list


def get_cat_name(lastCat, cat_alternative_names, show_alternate_names, show_missingno, lang):
    if (lastCat == "unknown_pet"):
        return LOCALIZED_STRINGS[str("missingno_" + show_missingno)][lang]
    if not show_alternate_names:
        return lastCat
    if cat_alternative_names == "":
        return lastCat
    length_names_cat_list = len(json.decode(cat_alternative_names)[lastCat])
    if length_names_cat_list == 0:
        return lastCat
    random_index = random.number(0, length_names_cat_list - 1)
    return json.decode(cat_alternative_names)[lastCat][random_index]

# ======================================
# Image / Pixel Art Helper
# ======================================
def index_of_cat_order(cat_order, name):
    for i in range(len(get_cat_order_list(cat_order))):
        if get_cat_order_list(cat_order)[i] == name:
            return i
    return -1


def get_cat_pixel_art_cat(cat_order, name, cat_0_art, cat_1_art, cat_2_art, cat_3_art, cat_0_custom_art, cat_1_custom_art, cat_2_custom_art, cat_3_custom_art):
    index = index_of_cat_order(cat_order, name)
    if index == 0:
        if cat_0_art == "custom":
            return cat_0_custom_art
        return CAT_ART["%d.png" % int(cat_0_art)]
    if index == 1:
        if cat_1_art == "custom":
            return cat_1_custom_art
        return CAT_ART["%d.png" % int(cat_1_art)]
    if index == 2:
        if cat_2_art == "custom":
            return cat_2_custom_art
        return CAT_ART["%d.png" % int(cat_2_art)]
    if index == 3:
        if cat_3_art == "custom":
            return cat_3_custom_art
        return CAT_ART["%d.png" % int(cat_3_art)]
    return CAT_ART["0.png"]  # Default


def render_cat_image(server_address, token, entity_toilet_name, show_pixel_art, cat_order, name, cat_0_art, cat_1_art, cat_2_art, cat_3_art, cat_0_custom_art, cat_1_custom_art, cat_2_custom_art, cat_3_custom_art, show_missingno):
    if name == "unknown_pet":
        return render.Image(src=base64.decode(UNKNOWN_PET_IMAGE[str(show_missingno) + ".png"]))
    if show_pixel_art == False:
        return render.Image(fetch_public_image(get_entity_picture_url(
            server_address, token, get_last_use(entity_toilet_name, server_address, token)[2])), width=16, height=16)
    img_bytes = get_cat_pixel_art_cat(
        cat_order, name, cat_0_art, cat_1_art, cat_2_art, cat_3_art, cat_0_custom_art, cat_1_custom_art, cat_2_custom_art, cat_3_custom_art)
    if img_bytes == "":
        return render.Image(src=base64.decode(CUSTOM_DEFAULT_IMAGE))
    return render.Image(src=base64.decode(img_bytes))


# ======================================
# Petkit Home Assistant Helper Functions
# ======================================
def get_last_used_entity_id(entity_toilet_name):
    return "sensor.%s_last_used_by" % entity_toilet_name


def get_wastebin_entity_id(entity_toilet_name):
    return "binary_sensor.%s_wastebin" % entity_toilet_name


def get_litter_level_entity_id(entity_toilet_name):
    return "sensor.%s_litter_level" % entity_toilet_name


def get_litter_entity_id(entity_toilet_name):
    return "binary_sensor.%s_litter" % entity_toilet_name


def get_last_use(entity_toilet_name, server_address, token):
    last_used_entity = get_last_used_entity_id(entity_toilet_name)
    name = ha_state_string(server_address, token,
                           last_used_entity, "unknown_pet")
    last_weight_id = "sensor.%s_latest_weight" % str(name).lower()
    last_weight = ha_state_float(
        server_address, token, last_weight_id, "?.?")
    return name, last_weight, last_weight_id


def get_wastebin_state(entity_toilet_name, server_address, token):
    wastebin_id = get_wastebin_entity_id(entity_toilet_name)
    state = ha_state_string(server_address, token, wastebin_id, "on")
    return state


def get_litter_level(entity_toilet_name, server_address, token):
    litter_level_id = get_litter_level_entity_id(entity_toilet_name)
    level = ha_state_float(server_address, token, litter_level_id, "0")
    return str(int(level))


def get_litter_state(entity_toilet_name, server_address, token):
    litter_id = get_litter_entity_id(entity_toilet_name)
    state = ha_state_string(server_address, token, litter_id, "on")
    return state


# ======================================
# Global Helper
# ======================================
def current_hour():
    return int(time.now().hour)


def normalize_server(server_address):
    s = "%s" % server_address
    if not (s.startswith("http://") or s.startswith("https://")):
        s = "http://" + s
    if s.endswith("/"):
        s = s[:-1]
    return s


def http_body(resp):
    b = resp.body
    if type(b) != "string":
        b = b()
    return b


def ha_get_state_json(server_address, token, entity_id):
    base = normalize_server(server_address)
    url = "%s/api/states/%s" % (base, entity_id)
    r = http.get(
        url,
        headers={"Authorization": "Bearer %s" % token},
    )
    raw = http_body(r)
    # Check: must be JSON
    if raw == "" or raw[0] != "{":
        # optional Debug:
        print("HA response not JSON:", raw)
        return {}
    return json.decode(raw)


def ha_state_float(server_address, token, entity_id, default):
    j = ha_get_state_json(server_address, token, entity_id)
    s = j.get("state", None)
    if s in [None, "unknown", "unavailable", ""]:
        return default
    return float(s)


def ha_state_string(server_address, token, entity_id, default):
    j = ha_get_state_json(server_address, token, entity_id)
    s = j.get("state", None)
    if s in [None, "unknown", "unavailable", ""]:
        return default
    return s


def get_entity_picture_url(server_address, token, entity_id):
    j = ha_get_state_json(server_address, token, entity_id)
    attrs = j.get("attributes", {})
    pic = attrs.get("entity_picture", None)
    if pic in [None, ""]:
        return None
    base = normalize_server(server_address)
    if pic.startswith("http://") or pic.startswith("https://"):
        return pic
    return base + pic


def fetch_public_image(url):
    r = http.get(url)
    b = r.body
    if type(b) != "string":
        b = b()
    return b
